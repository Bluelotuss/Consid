@model ConsidSite.Companies

@{
    ViewData["Title"] = "Details";
}

<h1>Company details</h1>
<a asp-action="Index">Back to company index</a>

<div>
    <h4>Store</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Name)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.OrganizationNumber)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.OrganizationNumber)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Notes)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Notes)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Stores)
        </dt>
        <dd class="col-sm-10">
            @foreach (var item in Model.Stores)
            {
                @Html.DisplayFor(model => item.Name)<br />
            }
        </dd>
    </dl>
</div>